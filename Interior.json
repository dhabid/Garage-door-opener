{
  "accessory": "HttpAdvancedAccessory",
  "service": "GarageDoorOpener",
  "name": "Interior",
  "forceRefreshDelay": 5,
  "username": "David",
  "password": "Aj89@AkLIEMnzer41",
  "debug": false,
  "urls": {
    "getCurrentDoorState": {
      "url": "http://192.168.1.234/?getstatus",
      "mappers": [
        {
          "type": "jpath",
          "parameters": {
            "jpath": "$.currentState",
            "index": 0
          }
        }
      ]
    },
    "getTargetDoorState": {
      "url": "http://192.168.1.234/?getstatus",
      "mappers": [
        {
          "type": "jpath",
          "parameters": {
            "jpath": "$.targetState",
            "index": 0
          }
        }
      ]
    },
    "setTargetDoorState": {
      "url": "http://192.168.1.234/?${value==0?'open':'close'}",
      "mappers": [
        {
          "type": "jpath",
          "parameters": {
            "jpath": "$.targetState",
            "index": 0
          }
        }
      ]
    }
  }
}